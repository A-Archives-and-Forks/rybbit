{
  "name": "@rybbit/analytics-script",
  "version": "1.0.0",
  "description": "Rybbit Analytics tracking script built with TypeScript",
  "main": "dist/script.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "dev": "vite build --watch",
    "clean": "rimraf dist",
    "pack-script": "npm run clean && npm run build && npm run build:dev"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "jsdom": "^23.0.0"
  },
  "dependencies": {
    "web-vitals": "^3.5.0"
  },
  "keywords": [
    "analytics",
    "tracking",
    "web-vitals",
    "typescript"
  ],
  "author": "Rybbit Analytics",
  "license": "MIT"
}
